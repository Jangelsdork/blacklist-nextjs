// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Promoter {
  id          Int           @id @default(autoincrement())
  first String
  last String
  company String
  email String
  phone String
  country String
  description String
}
